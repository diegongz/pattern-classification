    "def LOO(lst,alg):\n",
    "    for i in range(len(lst)):\n",
    "        df = lst.copy() #copy of the original df\n",
    "        \n",
    "        #test data_set\n",
    "        test_ds = df[i]\n",
    "        train_ds = df\n",
    "        \n",
    "        alg(test_ds,train_ds) "
